

for file in  sqme.c service.c width_2.c  
do
   if(test -r $file) then 
     $CC -c  $CFLAGS -I$CALCHEP/include $file
     if(test $? -ne 0) then exit 1; else rm  $file 
     fi
   fi
done

oo=
for file in  sqme.o service.o width_2.o
do
  if(test -r $file) then  oo="$oo $file";fi
done

if(test  -n "$oo" ) then
   ar r dproclib_0.a $oo; if(test $? -ne 0) then exit 1; else rm $oo; fi
fi


$CALCHEP/100 "$CC  $CFLAGS -I$CALCHEP/include" d 
if(test $? -ne 0) then exit 1; fi

$CALCHEP/100 "$CC  $CFLAGS -I$CALCHEP/include" f
if(test $? -ne 0) then exit 1; fi

$CALCHEP/100 "$CC $CFLAGS -I$CALCHEP/include"   c
if(test $? -ne 0) then exit 1; fi

exit 0
